<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003" InitialTargets="CheckSubnauticaPath">
  <PropertyGroup>
    <SubnauticaPath>D:\SteamLibrary\steamapps\common\Subnautica\Subnautica_Data\Managed</SubnauticaPath>
  </PropertyGroup>
  <Target Name="SubnauticaPathInvalid" Condition="!Exists($(SubnauticaPath))">
    <Error Text="The Subnautica Managed folder path is invalid - please run the BuildHelper in this directory!" />
  </Target>
  <Target Name="CheckSubnauticaPath" DependsOnTargets="SubnauticaPathInvalid" Condition="!Exists($(SubnauticaPath))">
  </Target>
  <PropertyGroup Condition="$(Configuration) == 'Debug' and $(OutputType) == 'Library' and '$(MSBuildProjectName)' != 'NitroxTest'">
    <OutputPath>$(SolutionDir)\bin\Debug\</OutputPath>
  </PropertyGroup>
  <PropertyGroup Condition="$(Configuration) == 'Release' and $(OutputType) == 'Library' and '$(MSBuildProjectName)' != 'NitroxTest'">
    <OutputPath>$(SolutionDir)\bin\Release\</OutputPath>
  </PropertyGroup>
  <!--<Import Project="$(SolutionDir)PostBuild.targets" />-->
</Project>